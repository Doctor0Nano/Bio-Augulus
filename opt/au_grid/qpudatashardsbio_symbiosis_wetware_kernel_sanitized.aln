# --------------------------------------------------------------------
# 2. Safety envelopes (no real wetware writes)
# --------------------------------------------------------------------
bio_safety_envelopes.aln
  envelope tissue_depth_mm
    mode            NonInvasive
    max_depth_mm    1.0
    hard_cap_mm     1.5
    enforcement     "Reject any configuration with depth_mm > max_depth_mm."
    on_violation    "Disable session, notify safety_officer, log to HyperledgerVital."[file:4][file:5]

  envelope rf_emf_limits
    band_profile    "ELF 10–1000 Hz; RF per FCC Part 15 reference tables."
    max_field_uT    100
    control_mode    "Monitoring-only for human subjects; no actuation."
    on_violation    "Drop RF-related command, enter SafeVisualMode, raise compliance_alert."[file:4]

  envelope bio_data_privacy
    pii_scope       "Neural-style signals, biosignals, behavioral logs."
    minimization    "Only derived metrics (scores, features, entropy) leave device."
    storage_policy  "Raw biosignal stored only in encrypted clinical or research vaults with IRB approval."
    regs            [GDPR, HIPAA, NeuralPrivacyActs, State-NeuroLaws]
    on_violation    "Freeze export job, quarantine dataset, require DPO + IRB review."[file:5]

  envelope substrate_restrictions
    allowed_substrate   "Digital simulation, commodity silicon hardware, virtual labs."
    prohibited_substrate "In vivo organoids, implanted scaffolds, unapproved tissue cultures."
    oversight_required  "Scientific-IRB + national ethics committee for any ex-vivo or clinical trial."
    default_mode        "Simulated-only; no lab equipment control unless safety_kernel=true and regulator_approved=true."[file:4][file:5]

